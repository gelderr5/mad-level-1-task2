<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tvTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/title"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/tvR0C1"/>

    <TextView
        android:id="@+id/tvR0C0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/a"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvTitle"
        app:layout_constraintEnd_toStartOf="@id/tvR0C1"
        app:layout_constraintBottom_toTopOf="@id/tvR1C0"/>

    <TextView
        android:id="@+id/tvR0C1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/b"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/tvR0C0"
        app:layout_constraintTop_toBottomOf="@id/tvTitle"
        app:layout_constraintEnd_toStartOf="@id/tvR0C2"
        app:layout_constraintBottom_toTopOf="@id/tvR1C1"/>

    <TextView
        android:id="@+id/tvR0C2"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="@string/ab"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/tvR0C1"
        app:layout_constraintTop_toBottomOf="@id/tvTitle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/tvR1C1"/>

    <TextView
        android:id="@+id/tvR1C0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/t"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvR0C0"
        app:layout_constraintEnd_toStartOf="@id/tvR1C1"
        app:layout_constraintBottom_toTopOf="@id/tvR2C0"/>

    <TextView
        android:id="@+id/tvR1C1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/t"
        android:textSize="25sp"
        app:layout_constraintStart_toEndOf="@id/tvR1C0"
        app:layout_constraintTop_toBottomOf="@id/tvR0C1"
        app:layout_constraintEnd_toStartOf="@id/etR1C2"
        app:layout_constraintBottom_toTopOf="@id/tvR2C1"/>

    <EditText
        android:id="@+id/etR1C2"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textSize="25sp"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@id/tvR0C1"
        app:layout_constraintTop_toBottomOf="@id/tvR0C1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/tvR2C1"
        android:importantForAutofill="no"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/tvR2C0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/t"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvR1C0"
        app:layout_constraintEnd_toStartOf="@id/tvR2C1"
        app:layout_constraintBottom_toTopOf="@id/tvR3C0"/>

    <TextView
        android:id="@+id/tvR2C1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/f"
        android:textSize="25sp"
        app:layout_constraintStart_toEndOf="@id/tvR2C0"
        app:layout_constraintTop_toBottomOf="@id/tvR1C1"
        app:layout_constraintEnd_toStartOf="@id/etR2C2"
        app:layout_constraintBottom_toTopOf="@id/tvR3C1"/>

    <EditText
        android:id="@+id/etR2C2"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textSize="25sp"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@id/tvR2C1"
        app:layout_constraintTop_toBottomOf="@id/tvR1C1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/tvR3C1"
        android:importantForAutofill="no"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/tvR3C0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/f"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvR2C0"
        app:layout_constraintEnd_toStartOf="@id/tvR3C1"
        app:layout_constraintBottom_toTopOf="@id/tvR4C0"/>

    <TextView
        android:id="@+id/tvR3C1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/t"
        android:textSize="25sp"
        app:layout_constraintStart_toEndOf="@id/tvR3C0"
        app:layout_constraintTop_toBottomOf="@id/tvR2C1"
        app:layout_constraintEnd_toStartOf="@id/etR3C2"
        app:layout_constraintBottom_toTopOf="@id/tvR4C1"/>

    <EditText
        android:id="@+id/etR3C2"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textSize="25sp"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@id/tvR3C1"
        app:layout_constraintTop_toBottomOf="@id/tvR2C1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/tvR4C1"
        android:importantForAutofill="no"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/tvR4C0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/f"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvR3C0"
        app:layout_constraintEnd_toStartOf="@id/tvR4C1"
        app:layout_constraintBottom_toTopOf="@id/btnSubmit"/>

    <TextView
        android:id="@+id/tvR4C1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/f"
        android:textSize="25sp"
        app:layout_constraintStart_toEndOf="@id/tvR4C0"
        app:layout_constraintTop_toBottomOf="@id/tvR3C1"
        app:layout_constraintEnd_toStartOf="@id/etR4C2"
        app:layout_constraintBottom_toTopOf="@id/btnSubmit"/>

    <EditText
        android:id="@+id/etR4C2"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textSize="25sp"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@id/tvR4C1"
        app:layout_constraintTop_toBottomOf="@id/tvR3C1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btnSubmit"
        android:importantForAutofill="no"
        tools:ignore="LabelFor" />

    <Button
        android:id="@+id/btnSubmit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/submit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvR4C1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>